CREATE TABLE attempts (id INT UNSIGNED NOT NULL, ip VARCHAR(48) NOT NULL, expiredate VARCHAR(32) NOT NULL, INDEX name_index (id), PRIMARY KEY(id)) DEFAULT CHARACTER SET utf8 COLLATE utf8_unicode_ci ENGINE = InnoDB;
CREATE TABLE userinfo (uuid VARCHAR(255) NOT NULL, extuid VARCHAR(255) NOT NULL, auth_source_name VARCHAR(25) NOT NULL, sub VARCHAR(50) DEFAULT NULL, name VARCHAR(25) DEFAULT NULL, given_name VARCHAR(25) DEFAULT NULL, family_name VARCHAR(25) DEFAULT NULL, middle_name VARCHAR(25) DEFAULT NULL, nickname VARCHAR(25) DEFAULT NULL, preferred_username VARCHAR(50) DEFAULT NULL, profile VARCHAR(255) DEFAULT NULL, email VARCHAR(50) DEFAULT NULL, email_verified VARCHAR(50) DEFAULT NULL, gender INT UNSIGNED DEFAULT 0 NOT NULL, birthdate DATETIME DEFAULT NULL, zoneinfo VARCHAR(20) DEFAULT NULL, locale VARCHAR(12) DEFAULT NULL, phone_number VARCHAR(20) DEFAULT NULL, address VARCHAR(255) DEFAULT NULL, preferred_lang VARCHAR(10) DEFAULT NULL, preferred_theme VARCHAR(25) DEFAULT NULL, updated_at DATETIME NOT NULL, roles VARCHAR(255) DEFAULT NULL, state VARCHAR(25) NOT NULL, lastlogon DATETIME DEFAULT NULL, logon_count INT UNSIGNED DEFAULT 0, send_confirm_count INT UNSIGNED DEFAULT 0, forget_pw_count INT UNSIGNED DEFAULT 0 NOT NULL, login_failed_count INT UNSIGNED DEFAULT 0 NOT NULL, activation_code VARCHAR(25) DEFAULT NULL, activation_code_lifetime DATETIME DEFAULT NULL, lock_time INT UNSIGNED DEFAULT 0, created_at DATETIME NOT NULL, INDEX name_index (name), PRIMARY KEY(uuid)) DEFAULT CHARACTER SET utf8 COLLATE utf8_unicode_ci ENGINE = InnoDB;
